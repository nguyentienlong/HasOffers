{
    "name": "grasp/hasoffers",
    "description": "A HasOffers Client for Grasp",
    "keywords": ["api", "Grasp", "Grasp-HasOffers", "HasOffers", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "grasp/grasp": "dev-master"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Grasp\\HasOffers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grasp\\Tests\\HasOffers\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
